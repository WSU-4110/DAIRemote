  </ItemGroup>
  
  <ItemGroup>
    <None Update="Resources\DAIRemoteLogo.ico">
	  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>